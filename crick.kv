<NameScreen>:
    name:'name'
    FloatLayout:
        id:main
        canvas:
            Rectangle:
                pos: self.pos
                size: self.size
                source: "11.jpg"
        TextInput:
            id:t78
            pos_hint:{'x':0,'y':0.95}
            text:'Enter Your Name'
            multiline:False
            size_hint:1,0.05
        Button:
            id:b1
            text:'Submit'
            font_size:10
            size_hint:0.2,0.08
            pos_hint:{'x':0.45,'y':.73}
            on_release:root.submit(t78.text)
<MainScreen>:
    name:'main'
    on_pre_enter: root.dis()
    l1289:l1234
    FloatLayout:
        id:main
        l123:l1234
        canvas:
            Rectangle:
                pos: self.pos
                size: self.size
                source: "11.jpg"
        Button:
            id:b1
            text:'Play'
            font_size:10
            size_hint:0.2,0.08
            pos_hint:{'x':0.45,'y':.73}
            on_release:app.root.current='choose'
            Image:
                source: 'enter.jpg'
                y: self.parent.y
                x: self.parent.x
                size:self.parent.size
                allow_stretch: True
        Button:
            id:b1
